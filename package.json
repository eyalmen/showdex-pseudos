    "react-inlinesvg": "^3.0.1",
    "react-redux": "^8.0.5",
    "react-select": "^5.4.0",
    "remove": "^0.1.5",
    "simplebar": "^5.3.9",
    "slugify": "^1.6.5",
    "ua-parser-js": "^1.0.32",
    "use-debouncy": "^4.3.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
