    "@use-gesture/react": "^10.2.22",
    "base-64": "^1.0.0",
    "chalk": "^5.1.2",
    "change-case": "^4.1.2",
    "classnames": "^2.3.2",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "final-form": "^4.20.7",
    "lzutf8": "^0.6.3",
    "qrcode.react": "^3.1.0",
    "qs": "^6.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-final-form": "^6.5.9",
    "react-hot-loader": "^4.13.0",
